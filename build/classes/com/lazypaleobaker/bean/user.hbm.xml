<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping  package="com.lazypaleobaker.bean">
<class name="Recipe" table="RECIPE">
<id column="RecipeID" name="recipeID" type="java.lang.Integer" />
<property column="RecipeName" name="recipeName" type="java.lang.String" />
<property column="Directions" name="directions" type="string" />
<property column="Notes" name="notes" type="java.lang.String" />
</class>
</hibernate-mapping>
